div(id='property-materials', class='hidden')
  div(class='row')
    label Materials
  div(class='row')
    form(class='form',role='form')
      {% csrf_token %}
      div(class='btn-group')
        button(type='button',class='btn btn-sm dropdown-toggle',data-toggle="dropdown") Action 
          span(class='caret')
        ul(class='dropdown-menu',role='menu') 
          li
            a(id='new-node',href='#',onclick='MaterialsControl.Add()') Add Material 

div(id='property-material', class='hidden')
  div(class='row')
    label Material
  div(class='row')
    form(class='form',role='form')
      {% csrf_token %}
      div(class='btn-group')
        button(type='button',class='btn btn-sm dropdown-toggle',data-toggle="dropdown") Action 
          span(class='caret')
        ul(class='dropdown-menu',role='menu') 
          li
            a(id='new-node',href='#',onclick='MaterialControl.AddComponent()') Add Component  
          li
            a(id='delete-node',href='#',onclick='MaterialControl.MaterialDelete()') Delete Current  
  div(class='row')
    form(class='form',role='form',action='javascript:void(0);')
      {% csrf_token %}
      div(class='form-group')
        div(class='input-group input-group-sm')
          span(class='input-group-addon') Name:
          input(class='form-control',name='name',onchange='MaterialControl.NameChanged(this)')
        div(class='input-group input-group-sm')
          span(class='input-group-addon') Density(g/cm3):
          input(type='number',step='0.0001',class='form-control',name='density',onchange='MaterialControl.DensityChanged(this)')
        div(class='input-group input-group-sm')
          span(class='input-group-addon') Type:
          select(class='form-control',name='type',onchange='MaterialControl.TypeChanged(this)')
              option mixture
              option element
        div(class='input-group input-group-sm')
          span(class='input-group-addon') Mix by:
          select(class='form-control',name='weight',onchange='MaterialControl.WeightChanged(this)')
              option fraction
              option composite

div(id='property-component', class='hidden')
  div(class='row')
    label Component
  div(class='row')
    form(class='form',role='form')
      {% csrf_token %}
      div(class='btn-group')
        button(type='button',class='btn btn-sm dropdown-toggle',data-toggle="dropdown") Action 
          span(class='caret')
        ul(class='dropdown-menu',role='menu') 
          li
            a(id='delete-node',href='#',onclick='ComponentControl.DeleteComponent()') Delete Current  
  div(class='row')
    form(class='form',role='form',action='javascript:void(0);')
      {% csrf_token %}
      div(class='form-group')
        div(class='input-group input-group-sm')
          span(class='input-group-addon') Name:
          input(class='form-control',name='name',disabled='disabled')
        div(class='input-group input-group-sm')
          span(class='input-group-addon') Weight:
          input(type='number',step='0.0001',class='form-control',name='weight',onchange='ComponentControl.WeightChanged(this)')

div(id='dialog-input-name',class='ui-widget', title='Dialog')
  p(class="validateTips") Input Name:
  form
    fieldset
      label(for="name")Name
      input(type="text", name="name", class="text ui-widget-content ui-corner-all")

div(id='dialog-select-name',class='ui-widget', title='Dialog')
  p(class="validateTips") Select
  form
    fieldset
      label(for="name")Name
      select(name="name", class="ui-widget-content ui-corner-all")
        option None
