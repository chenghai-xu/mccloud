div(id='property-solid-box', class='hidden')
  div(class='row')
    label Box
  div(class='row')
    //form(class='form',role='form',action='javascript:void(0);',onsubmit='OnBoxSubmit(this)')
    form(class='form',role='form',action='javascript:void(0);')
      div(class='form-group')
        div(class='input-group input-group-sm')
          span(class='input-group-addon') x:
          input(type='number',step='0.0001',class='form-control',name='x',onchange='SolidBox.ValueChanged(this)')
        div(class='input-group input-group-sm')
          span(class='input-group-addon') y:
          input(type='number',step='0.0001',class='form-control',name='y',onchange='SolidBox.ValueChanged(this)')
        div(class='input-group input-group-sm')
          span(class='input-group-addon') z:
          input(type='number',step='0.0001',class='form-control',name='z',onchange='SolidBox.ValueChanged(this)')
        div(class='input-group input-group-sm')
          span(class='input-group-addon') lunit:
          select(class='form-control',name='lunit',onchange='SolidBox.LUnitChanged(this)')
              option m
              option cm
              option mm
              option km
      //button(type='submit',class="btn btn-default") Save

